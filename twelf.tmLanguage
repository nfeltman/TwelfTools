<?xml version="1.0" encoding="UTF-8"?>
<plist version="1.0">
<dict>
	<key>fileTypes</key>
	<array>
		<string>elf</string>
	</array>
	<key>name</key>
	<string>Twelf</string>
	<key>patterns</key>
	<array>
		<dict>
			<key>comment</key>
			<string>Twelf keywords</string>
			<key>match</key>
			<string>\b(type)\b</string>
			<key>name</key>
			<string>keyword.source.twelf</string>
		</dict>
		<dict>
			<key>comment</key>
			<string>Twelf directives</string>
			<key>match</key>
			<string>\b(%mode|%name)\b</string>
			<key>name</key>
			<string>directive.source.twelf</string>
		</dict>
		<dict>
			<key>begin</key>
			<string>\%\{</string>
			<key>comment</key>
			<string>Block Comment</string>
