			<string>\b[A-Z]([A-z0-9\']*)\b</string>
			<key>name</key>
			<string>support.metavariable.source.twelf</string>
		</dict>
		<dict>
			<key>comment</key>
			<string>Local Variables</string>
			<key>match</key>
			<string>\b[a-z]([A-z0-9\']*)\b</string>
			<key>name</key>
			<string>support.variable.twelf</string>
		</dict>
